/* * * * * * * * * * * * * * * * * * * * * * * * * * *


                IMAGE GRID LAYOUT SECTION


* * * * * * * * * * * * * * * * * * * * * * * * * * */

.larger-image-container , .smaller-image-container{
  background-size: cover;
  background-position: center;
  background-repeat: no-repeat;
}

.smaller-image-container{
  background-size: cover;
  background-position: left;
  background-repeat: no-repeat;
}

.larger-image-container{
  height: 600px;
}

.smaller-image-container, .grid-text-content{
  height: 285px;
}

.grid-text-content{
  background-color: $deepblue;
  margin-top: 30px;
  overflow: hidden;
}

.grid-text-content p{
  font-family: 'Cormorant Garamond';
  color: white;
  font-weight: bold;
  font-size: 25px;
  margin-bottom: 0px;
  // margin-left: 20px;
}

.grid-text-content-wrapper{
  height: 100%;
  padding: 54px 50px 28px 50px;
}

.border-line{
  border-right: 4px solid white;
  height: 80%;
}

.quote-marks img{
    max-width: 40px;
}

.quote-marks{
  position: relative;
  left: 13px;
}

.larger-image-half{
  width: 65%;
  display: inline-block;
}

.image-and-text-half{
  width: 33%;
  float: right;
}

.grid-layout-container{
  height: auto;
  margin-top: 100px !important;
  margin-bottom: 100px !important;
  width: 80%;
  margin: 0 auto;
  display: block;
}

.quote-line-half{
  width: 10%;
  float: left;
  height: 70%;
}

.quote-content-half{
  width: 90%;
  float: right;
  padding-left: 24px;
}

