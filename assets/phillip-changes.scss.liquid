/* * * * * * * * * * * * * * * * * * * * * * * * * * *

              RESET  BODY STYLING


* * * * * * * * * * * * * * * * * * * * * * * * * * */

$mainbrown: #EBECE4;
$deepblue: #38373E;
$olivegreen: #AAB7B4;

body{
  background-color: white;
}

.top_bar{
  background-color: $mainbrown;
}

.footer{
  background-color: $mainbrown;
}

.hide{
  display: none;
}

#mobile_menu .sublink ul{
  display: block;
}

.caption.captionOverlay--true .headline, .caption.captionOverlay--true .subtitle{
  text-shadow: 0px 0px 0px rgba(0,0,0,0.3);
}


/* * * * * * * * * * * * * * * * * * * * * * * * * * *


              RESET  GRID STYLING


* * * * * * * * * * * * * * * * * * * * * * * * * * */



 .container .full-width-cgc{
   width: 100%;
 }

 .container .one-half-cgc{
   width: 50%;
 }

 .container .one-thirds-cgc{
   width: 33%;
   display: inline-block;
   margin: 0px;
 }


/* * * * * * * * * * * * * * * * * * * * * * * * * * *


                     NAVIGATION


* * * * * * * * * * * * * * * * * * * * * * * * * * */
.cgc-main-nav{
  display: block;
  background: white;
  min-height: 75px;
  float: none !important;
  margin: 0 auto;
  // width: 86% !important;
}

.cgc-main-nav .cgc-menu-items{
  float: right;
  margin-right: 0px;
}

.cgc-down-arrow{
  padding: 0px;
  position: relative;
  top: -3px;
  font-size: 21px;
  padding: 0px;
}

// .nav{
//   width: 93% !important;
// }

.nav{
  width: 65% !important;
}

.menu li a[href]:hover, .menu li a:focus, .menu a[href]:hover span{
  color: black;
}

.menu a.top_link:hover, .menu a.dropdown_link:hover, .menu a.active_link{
  border: 0px solid black;
}
.cgc-main-nav{
a.top_link{
  padding: 0px;
  }
}

.cgc-main-nav .main_nav{
  padding-top: 30px !important;
}

.menu a.top_link, .menu a.dropdown_link{
  padding: 0px;
  font-weight: 500;
}


.bottom-nav span{
  display: block;
}

.cgc-dropdown{
  border-bottom: 6px solid $mainbrown;
  min-height: 200px;
}

.menu-item-box{
  margin: 0px 10px 16px 10px;
  display: inline-block;
  padding: 40px;
  min-height: 80px;
  transition: all 0.3s ease-in-out;
}

.menu-item-box:hover{
  background-size: 120% !important;
}

.menu-item-box a{
  font-family: 'Cormorant Garamond';
  color: white;
  font-weight: bold;
  font-size: 25px;
  display: block;
}

.explore-line{
  width: 0%;
  border-bottom: 1px solid white;
  display: inline-block;
  position: relative;
  bottom: 4px;
}


.explore-container{
  display: inline-block;
  width: 50%;
}

.submenu-text-content{
  // width: 100%;
  display: inline-block;
}

.submenu-text-content a{
  display: block;
  padding-bottom: 5px;
  color: white !important;
}

.submenu-text-content:active{
  font-family: 'Cormorant Garamond';

}

.menu-item-box p{
  color: white;
  font-weight: 500;
  display: inline;
  padding: 0px;
}


.dropdown_column.menu-item-box.four.columns{
  width: 250px !important;
}

.bottom-menu-wrapper{
  margin: 0 auto;
  width: fit-content;
}

.CGC-logo{
  display: inline;
  float: left;
  top: 12px;
}

.cgc-menu-items li{
   margin-left: 35px;
}

.logo{
  font-size: 23px;
}

