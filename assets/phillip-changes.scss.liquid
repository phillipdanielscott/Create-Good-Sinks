* * * * * * * * * * * * * * * * * * * * * * * * * * */


.owl-carousel .item{
  width: 100%;
  // border: 2px solid black;
  display: block;
  width: 100%;
  height: -webkit-fill-available;
}


.cgc-carousel-wrapper{
  // height: 800px;
  // height: 975px;
  height: 1100px;
  // padding: 0px 50px;
  margin: 0px 0px 80px 0px;
  }

.cgc-carousel-wrapper h2{
  font-size: 130px;
  margin: 0px;
  line-height: 7rem;
  margin-bottom: 35px;
}

.cgc-carousel-wrapper p{
  font-weight: 500;
  font-size: 20px;
}

.full-item img{
  max-width: 750px;
  position: absolute;
  right: 27vw;
  top: 50%;
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
  // width: 50vmin;
  -webkit-perspective: 100vmin;
  perspective: 100vmin;
  -webkit-filter: drop-shadow(12px 12px 7px rgba(0,0,0,0.5));
  filter: drop-shadow(12px 12px 7px rgba(0,0,0,0.5));
}

.featured-image-container{
 height: -webkit-fill-available;
}

.cgc-carousel-wrapper .carousel-text-underline{
  width: 22%;
  margin-bottom: 20px;
    transition: all 0.3s ease-in-out;
}

.owl-carousel .owl-item .featured-bg-color{
  height: -moz-available;          /* WebKit-based browsers will ignore this. */
  height: -webkit-fill-available;  /* Mozilla-based browsers will ignore this. */
  // height: fill-available;
  position: absolute;
  right: 0;
  top: 0;
  bottom: 0;
  width: 80%;
  -webkit-transform: scaleX(.42);
  transform: scaleX(0.42);
  background-color: var(--backgroundColor);
  -webkit-transform-origin: right;
  transform-origin: right;
  margin-right: 336px;
}

.carousel-text-content{
  position: absolute;
  top: 50%;
  left: 278px;
  padding-left: 60px;
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
  width: 50vmin;
  -webkit-perspective: 100vmin;
  perspective: 100vmin;
  text-align: left;
}

.featured-sink-item img{
  max-width: 1200px;
  position: absolute;
  right: 10vw;
  top: 50%;
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
  -webkit-perspective: 100vmin;
  perspective: 100vmin;
}

.featured-sink-container .container{
  height: -webkit-fill-available;
  width: 65%;
}

.featured-sink-bg{
  height: 85%;
  position: absolute;
  top: 6%;
  width: 100%;
}

.featured-sink-container h3{
  font-size: 65px;
}

.featured-sink-container .offset-text-top,.featured-sink-container .offset-text-bottom{
  position: absolute;
}

.featured-sink-container .offset-text-top{
  top: 9px;
}

.featured-sink-container .offset-text-bottom{
  bottom: 37px;
  right: 0px;
}

.borderline-left{
  height: 550px;
  width: 5px;
  position: absolute;
  bottom: 34%;
}

.borderline-right{
  height: 550px;
  width: 5px;
  position: absolute;
  bottom: 18%;
  right: 0px;
}

.owl-theme .owl-dots .owl-dot.active span,
.owl-theme .owl-dots .owl-dot:hover span{
  background: #38373E;
}

button.owl-dot.active span{
  border: 1px solid black;
}

.carousel-wrap {
    width: 1000px;
    margin: auto;
    position: relative;
  }

  .owl-carousel .item {
      text-align: center;
  }
  .owl-carousel .nav-btn{
    position: absolute;
    height: fit-content;
    width: 26px;
    cursor: pointer;
    // bottom: 50% !important;
    // top: 50% !important;
  }

  .owl-carousel .prev-slide,
  .owl-carousel .next-slide{
    bottom: 50% !important;
  }

  .owl-carousel .owl-prev.disabled,
  .owl-carousel .owl-next.disabled{
    pointer-events: none;
    opacity: 0.2;
  }

  .owl-carousel .prev-slide{
      background: url(nav-icon.png) no-repeat scroll 0 0;
      left: -70px;
  }
  .owl-carousel .next-slide{
      background: url(nav-icon.png) no-repeat scroll -24px 0px;
      right: -59px;
  }

  .owl-carousel .review-angle-left,
  .owl-carousel .review-angle-right{
    font-size: 26px;
    padding: 5px 10px;
    border: 1px solid black;
    border-radius: 60%;
  }

  .owl-carousel .prev-slide:hover{
     background-position: 0px -53px;
  }
  .owl-carousel .next-slide:hover{
    background-position: -24px -53px;
  }

  span.img-text {
    text-decoration: none;
    outline: none;
    transition: all 0.4s ease;
    -webkit-transition: all 0.4s ease;
    -moz-transition: all 0.4s ease;
    -o-transition: all 0.4s ease;
    cursor: pointer;
    width: 100%;
    font-size: 23px;
    display: block;
    text-transform: capitalize;
  }

  .owl-theme .owl-nav [class*=owl-]:hover{
    color: black;
  }

  .owl-nav{
    height: 0px;
  }


